public class Demo{
	public static void main(String[] args) {
		int []a = {12,56,78,12,56,34};
		System.out.println("排序前");
		for(int i=0;i<a.length;i++) {
			System.out.printf(" "+a[i]);
		}
		for(int i=1;i<a.length;i++) {
			int temp = a[i];
			for(int j = i-1;j>= 0;j--) {
				if(a[j]>temp) {
					a[j+1] = a[j];
				}else {
					break;
				}
				
			}a[j+1] = temp;
			
		}
		System.out.println("排序之后");
		for(int i=0;i<a.length;i++) {
			System.out.printf(a[i]+" ");
		}
	}
}